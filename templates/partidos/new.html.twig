{% extends 'base.html.twig' %}

{% block title %}New Partidos{% endblock %}

{% block body %}
<script>
var i = 0;
function move() {

  if (i == 0) {
    i = 1;
    var elem = document.getElementById("myBar");
    
    
    var width = 1;
    var id = setInterval(frame, 5);
    function frame() {
      if (width >= 100) {
        clearInterval(id);
        i = 0;
      } else {
        width++;
        elem.style.width = width + "%";
      }
    }
  }
}
</script>
  

    <h1>Create new Partidos</h1>

    {{ include('partidos/_form.html.twig') }}


    <a href="{{ path('partidos_index') }}">back to list</a>
{% endblock %}
